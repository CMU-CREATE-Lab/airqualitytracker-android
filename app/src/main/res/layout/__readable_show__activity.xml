<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:activity="org.cmucreatelab.tasota.airprototype.activities.readable_show.ReadableShowActivity"
    android:id="@+id/layoutShowAddress"
    android:background="#666">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="999"
                    android:id="@+id/textShowAddressAqiValue"
                    android:textSize="160dp"
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#fff" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="AQI    /500"
                    android:id="@+id/textShowAddressAqiLabel"
                    android:gravity="center_vertical|center_horizontal"
                    android:textSize="25dp"
                    android:textColor="#fff" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_weight="10" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0-50 AQI"
                    android:id="@+id/textShowAddressAqiRange"
                    android:gravity="center_vertical|center_horizontal"
                    android:textSize="20dp"
                    android:textColor="#fff" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Good"
                    android:id="@+id/textShowAddressAqiTitle"
                    android:gravity="center_vertical|center_horizontal"
                    android:textSize="14dp"
                    android:textColor="#000"
                    android:textStyle="bold" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Air quality is considered satisfactory, and air pollution poses little or no risk."
                    android:id="@+id/textShowAddressAqiDescription"
                    android:gravity="center_vertical|center_horizontal"
                    android:textSize="14dp"
                    android:textColor="#000"
                    android:paddingLeft="20dp"
                    android:paddingRight="20dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_weight="10" >

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:background="#10ffffff"
                android:id="@+id/frameLayout2" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <FrameLayout
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:layout_width="32dp"
                            android:layout_height="match_parent"
                            android:id="@+id/imageView8"
                            android:src="@drawable/ic_near_me_black_24dp"
                            android:layout_gravity="center" />
                    </FrameLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="3" >

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="# dirty days in the  past year"
                            android:id="@+id/textViewDirtyDays"
                            android:layout_gravity="left|center_vertical"
                            android:textColor="#000"
                            android:textSize="14dp"
                            android:layout_alignParentLeft="false"
                            android:layout_alignParentStart="true"
                            android:gravity="top"
                            android:layout_alignParentBottom="false"
                            android:textStyle="bold"
                            android:layout_centerVertical="true" />
                    </RelativeLayout>

                    <FrameLayout
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:layout_width="32dp"
                            android:layout_height="match_parent"
                            android:id="@+id/imageView9"
                            android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                            android:layout_gravity="center" />
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_weight="10" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:background="#10ffffff"
                android:id="@+id/frameAqiButton">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <FrameLayout
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <ImageView
                            android:layout_width="32dp"
                            android:layout_height="match_parent"
                            android:id="@+id/imageView6"
                            android:src="@drawable/ic_near_me_black_24dp"
                            android:layout_gravity="center" />
                    </FrameLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="3">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:text="READING FROM:"
                            android:id="@+id/textView32"
                            android:layout_gravity="left|top"
                            android:textColor="#000"
                            android:textSize="12dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:gravity="bottom" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Reading Name at Street AirNow"
                            android:id="@+id/textViewReadingName"
                            android:layout_gravity="left|center_vertical"
                            android:textColor="#000"
                            android:textSize="14dp"
                            android:layout_below="@+id/textView32"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:gravity="top"
                            android:layout_alignParentBottom="true"
                            android:textStyle="bold" />
                    </RelativeLayout>

                    <FrameLayout
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <ImageView
                            android:layout_width="32dp"
                            android:layout_height="match_parent"
                            android:id="@+id/imageView7"
                            android:src="@drawable/ic_keyboard_arrow_right_black_24dp"
                            android:layout_gravity="center" />
                    </FrameLayout>

                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp" >

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal" >

                    <FrameLayout
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:background="#600"
                        android:visibility="invisible" />

                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal" >

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:id="@+id/imageView2"
                        android:src="@drawable/twitterlogo_small_white" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Join the conversation @SpeckSensor"
                        android:id="@+id/textView9"
                        android:textColor="#fff"
                        android:textSize="14dp" />
                </LinearLayout>
            </FrameLayout>

        </LinearLayout>
    </ScrollView>
</RelativeLayout>
